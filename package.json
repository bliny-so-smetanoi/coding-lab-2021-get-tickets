{
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "name": "coding-lab-project",
  "version": "1.0.0",
  "description": "Coding Lab Project 2021",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix frontend",
    "client:install": "npm install --prefix frontend",
    "client:build": "npm run build --prefix frontend",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "node",
    "react",
    "express",
    "mongo"
  ],
  "author": "Akezhan Issadilov <akezhan.issadilov@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.1",
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.5",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  }
}
